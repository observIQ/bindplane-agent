{
  "product": "BindPlane Distro for OpenTelemetry Collector (BDOT)",
  "company": "observIQ, Inc.",
  "license": "./LICENSE.rtf",
  "info": {
    "help-link": "https://observiq.com/contact/",
    "support-link": "https://observiq.com/contact/"
  },
  "upgrade-code": "D67CCA1A-6708-4096-8BDE-5069739FB861",
  "files": [
    {
      "path": "opampsupervisor.exe",
      "service": {
        "name": "bindplane-otel-collector",
        "start": "delayed",
        "display-name": "BDOT",
        "description": "BindPlane Distro fo OpenTelemetry Collector (BDOT).",
        "arguments": "--config &quot;[INSTALLDIR]supervisor.yaml&quot;"
      }
    },
    {
      "path": "bindplane-otel-collector.exe"
    },
    {
      "path": "opentelemetry-java-contrib-jmx-metrics.jar"
    },
    {
      "path": "VERSION.txt"
    },
    {
      "path": "LICENSE"
    }
  ],
  "directories": [
    {
      "name": "plugins"
    },
    {
      "name": "storage"
    },
    {
      "name": "supervisor_storage"
    },
    {
      "name": "install"
    }
  ],
  "environments": [
    {
      "name": "OIQ_OTEL_COLLECTOR_STORAGE",
      "value": "[INSTALLDIR]storage",
      "permanent": "no",
      "system": "yes",
      "action": "set",
      "part": "all"
    },
    {
      "name": "OIQ_OTEL_COLLECTOR_HOME",
      "value": "[INSTALLDIR]",
      "permanent": "no",
      "system": "yes",
      "action": "set",
      "part": "all"
    }
  ],
  "registries": [
    {
      "path": "HKCU\\Software\\observiq\\observIQ OpenTelemetry Collector",
      "values": [
        {
          "name": "Version",
          "value": "v[ProductVersion]"
        },
        {
          "name": "InstallDir",
          "value": "[INSTALLDIR]"
        }
      ]
    },
    {
      "path": "HKLM\\Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\observIQ OpenTelemetry Collector",
      "values": [
        {
          "name": "NoModify",
          "value": "1",
          "type": "integer"
        }
      ]
    }
  ],
  "hooks": []
}

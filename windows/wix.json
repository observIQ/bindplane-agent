{
  "product": "observIQ OpenTelemetry Collector",
  "company": "observIQ, Inc.",
  "license": "./LICENSE",
  "info": {
    "help-link": "https://observiq.com/contact/",
    "support-link": "https://observiq.com/contact/"
  },
  "upgrade-code": "D67CCA1A-6708-4096-8BDE-5069739FB861",
  "files": [
    {
      "path": "observiq-otel-collector.exe",
      "service": {
        "name": "observiq-otel-collector",
        "start": "delayed",
        "display-name": "observIQ OpenTelemetry Collector",
        "description": "observIQ's distribution of the OpenTelemetry collector.",
        "arguments": "--config &quot;[INSTALLDIR]config.yaml&quot;"
      }
    },
    {
      "path": "config.yaml",
      "never_overwrite": true
    },
    {
      "path": "opentelemetry-java-contrib-jmx-metrics.jar"
    },
    {
      "path": "LICENSE"
    }
  ],
  "directories": [
    {
      "name": "plugins"
    }
  ],
  "registries": [
    {
      "path": "HKCU\\Software\\observiq\\observIQ OpenTelemetry Collector",
      "values": [
        {
          "name": "Version",
          "value": "v[ProductVersion]"
        },
        {
          "name": "InstallDir",
          "value": "[INSTALLDIR]"
        }
      ]
    },
    {
      "path": "HKLM\\Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\observIQ OpenTelemetry Collector",
      "values": [
        {
          "name": "NoModify",
          "value": "1",
          "type": "integer"
        }
      ]
    },
    {
      "path": "HKLM\\SYSTEM\\CurrentControlSet\\Services\\EventLog\\Application\\observIQ OpenTelemetry Collector",
      "values": [
        {
          "name": "TypesSupported",
          "type": "integer",
          "value": "7"
        }
      ]
    }
  ],
  "hooks": []
}
